# /!\ TWO DISTINCT SOLUTIONS GIVEN BELOW
# O(N+Q) approach, O(N) space
N,Q = map(int,input().split())
A = [0]*N
for _ in range(Q):
    L,R = map(int,input().split())
    A[L] += 1
    if R+1<N:
        A[R+1] -= 1
x = res = 0
for i in range(N):
    x += A[i]
    res += x%2
print(res)
quit()
# O(N + Q log Q) approach, O(Q) space
N,Q = map(int,input().split())
LR = []
for _ in range(Q):
    L,R = map(int,input().split())
    LR.append((L,1))
    LR.append((R+1,-1))
LR.sort()
j = cnt = res = 0
for i in range(N):
    while j<len(LR) and LR[j][0]<=i:
        cnt += LR[j][1]
        j += 1
    res += cnt%2
